journal = {Archives of Biochemistry and Biophysics},
keywords = {Amino Acid Sequence,Arabidopsis,Arabidopsis: enzymology,Arabidopsis: genetics,Enzyme Activation,Fatty Acids,Methylation,Methyltransferases,Methyltransferases: analysis,Methyltransferases: chemistry,Methyltransferases: genetics,Methyltransferases: metabolism,Molecular Sequence Data,Protein Engineering,Protein Engineering: methods,Recombinant Proteins,Recombinant Proteins: chemistry,Recombinant Proteins: metabolism,Unsaturated,Unsaturated: chemistry},
