issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Algorithms,Antineoplastic Agents,Antineoplastic Agents: pharmacology,Breast Neoplasms,Breast Neoplasms: drug therapy,Cell Line, Tumor,Drug Design,Female,Gene Expression Profiling,Humans,Neoplasms,Neoplasms: drug therapy,Neoplasms: genetics,Urinary Bladder Neoplasms,Urinary Bladder Neoplasms: drug therapy},
month = aug,
