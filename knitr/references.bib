pages = {785--92},
pmid = {15945375},
title = {{Validation of oligonucleotide microarray data using microfluidic low-density arrays: a new statistical method to normalize real-time RT-PCR data.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15945375},
volume = {38},
year = {2005}
}
