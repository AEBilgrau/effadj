file = {:C$\backslash$:/Documents and Settings/jywh/Lokale indstillinger/Application Data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bussey et al. - 2006 - Integrating data on DNA copy number with gene expression levels and drug sensitivities in the NCI-60 cell line panel.pdf:pdf},
issn = {1535-7163},
journal = {Molecular cancer therapeutics},
