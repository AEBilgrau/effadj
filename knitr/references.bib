issn = {0021-9258},
journal = {Journal of Biological Chemistry},
keywords = {Amino Acid,Amino Acid Sequence,Animals,CHO Cells,Chondroitin Sulfates,Chondroitin Sulfates: biosynthesis,Complementary,Cricetinae,DNA,DNA Primers,Gene Amplification,Humans,Isoenzymes,Isoenzymes: chemistry,Isoenzymes: genetics,Isoenzymes: metabolism,Molecular Sequence Data,N-Acetylgalactosaminyltransferases,N-Acetylgalactosaminyltransferases: chemistry,N-Acetylgalactosaminyltransferases: genetics,N-Acetylgalactosaminyltransferases: metabolism,Open Reading Frames,Polymerase Chain Reaction,Sequence Alignment,Sequence Homology,Transfection},
